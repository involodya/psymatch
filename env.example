# Скопируйте этот файл в .env и заполните своими значениями

# Токен бота из @BotFather
BOT_TOKEN=your_telegram_bot_token_here

# ID администраторов через запятую (узнать свой ID можно у @userinfobot)
ADMIN_IDS=123456789,987654321

# Путь к файлу базы данных
DATABASE_PATH=psymatch.db

# Путь к файлу логов
LOG_FILE=bot.log

# Настройки веб-админки
ADMIN_SECRET_KEY=your_secret_key_for_flask_sessions
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password_here

